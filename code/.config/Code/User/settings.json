{
    "workbench.colorTheme": "Solarized Light",
    "workbench.colorCustomizations": {
        "editorUnnecessaryCode.opacity": "#000000FF",
        "editorUnnecessaryCode.border": "#ff9100"
    },
    "gitlens.hovers.currentLine.over": "line",
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    },
    "git.autofetch": true,
    "git.openRepositoryInParentFolders": "always",
    "cmake.generator": "Ninja",
    "ninjabuild.ninjaBuildPath": "${workspaceFolder}/build/build.ninja",
    "ros.distro": "noetic",
    "cmake.installPrefix": "${workspaceFolder}/install/",
    "cmake.sourceDirectory": "${workspaceFolder}/src/",
    "cmake.skipConfigureIfCachePresent": true,
    "C_Cpp.default.compilerPath": "/usr/lib/ccache/clang",
    "C_Cpp.default.cppStandard": "c++17",
    "C_Cpp.default.cStandard": "c17",
    "cmake.configureOnOpen": false,
    "cmake.ignoreCMakeListsMissing": true,
    "cmake.loadCompileCommands": true,
    "cmake.buildDirectory": "${workspaceFolder}/build",
    "C_Cpp.files.exclude": {

        "**/.vscode": true,
        "**/.vs": true
    },
    "yaml.schemas": {
        "file:///home/nicu/.vscode/extensions/atlassian.atlascode-3.0.10/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "redhat.telemetry.enabled": false,
    "atlascode.jira.jqlList": [
        {
            "id": "b27ed0c5-ee78-4b25-9712-4e1e45c8024e",
            "enabled": true,
            "name": "My traclabs Issues",
            "query": "assignee = currentUser() AND resolution = Unresolved ORDER BY lastViewed DESC",
            "siteId": "2b7d7789-cd70-47ec-b8ab-b8ec9b923bd0",
            "monitor": true
        }
    ],
    "lldb.suppressUpdateNotifications": true,
    "editor.stickyScroll.enabled": true,
    "editor.scrollbar.horizontal": "visible",
    "editor.scrollbar.vertical": "visible",
    "workbench.editor.wrapTabs": true,
    "workbench.editor.highlightModifiedTabs": true,
    "launchqtcreator.qtDesignerPath": "/home/nicu/cartel2_ws/src/craftsman_application_gui/craftsman_application_gui/ui/rviz_craftsman_panel.ui",
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "julia.symbolCacheDownload": true,
    "[julia]": {
        "editor.defaultFormatter": "julialang.language-julia"
    },
    "remote.autoForwardPortsSource": "hybrid",
    "julia.enableTelemetry": false,
    "files.autoSave": "onFocusChange",
    "cmake.showOptionsMovedNotification": false,
    "codeium.enableConfig": {
        "*": true,
        "markdown": true,
        "xml": true
    },
    "ros.isolateEnvironment": true,
    "ros.rosSetupScript": "${workspaceFolder}/devel/setup.bash",
    "julia.cellDelimiters": [

        "^##(?!#)",
        "^#(\\s?)%%",
        "^#-"
    ],
    "cmake.preferredGenerators": [
        "Ninja"
    ],
    "julia.executablePath": "~/.juliaup/bin/julia",
    "git-graph.openToTheRepoOfTheActiveTextEditorDocument": true,
    "git-graph.maxDepthOfRepoSearch": 3,
    "git.repositoryScanMaxDepth": 3,
    "python.createEnvironment.trigger": "off",
    "files.trimTrailingWhitespace": true,
    "markdown.extension.theming.decoration.renderTrailingSpace": true,
    "editor.renderWhitespace": "trailing",
    "editor.minimap.enabled": false,
    "cmake.cmakePath": "/usr/bin/cmake",
    "ninjabuild.ninjaExecutable": "/usr/bin/ninja",
    "ninjabuild.testExecutablesPath": "${workspaceFolder}/devel/lib/",
    "ninjabuild.ninjaArguments": [
    ],
    "cmakeExplorer.buildDir": "build/",
    "editor.wordWrapColumn": 120,
    "editor.wrappingStrategy": "advanced",
    "workbench.quickOpen.closeOnFocusLost": false,
    "workbench.editor.limit.enabled": true,
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "cmake.configureOnStart": false,
    "cmake.cpptools.browseTargets": [

    ],
    "cmake.cpptools.intelliSenseMode": "gcc-x64",
    "cmake.cpptools.languageConfiguration.CXX": {

    },
    "cmake.automaticReconfigure": false,
    "dev.containers.mountWaylandSocket": false,
    "dev.containers.defaultExtensionsIfInstalledLocally": [
        "GitHub.vscode-pull-request-github"
    ],
    "dev.containers.dockerPath": "podman",
    "docker.dockerPath": "/usr/bin/podman",
    "docker.commands.build": "${containerCommand} build --pull --rm -f \"${dockerfile}\" -t ${tag} \"${context}\""
}
